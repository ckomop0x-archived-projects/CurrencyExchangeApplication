{
  "extends": "airbnb",
  "rules": {
    "comma-dangle": 0,
    "no-tabs": 0,
    "indent": [2,"tab",
      {
        "SwitchCase": 1
      }
    ],
    "react/jsx-indent": [2,"tab"],
    "react/jsx-indent-props": [2,"tab"],
    "react/jsx-filename-extension": 0,
    "react/require-default-props": 0,
    "react/forbid-prop-types": 0,
    "react/jsx-no-bind": 0,
    "import/prefer-default-export": 0,
    "no-underscore-dangle": 0
  }
}